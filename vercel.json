{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://controle-producao-backend-1.onrender.com/api/$1"
        },
        {
            "source": "/((?!assets/|favicon.ico|sw.js|manifest.webmanifest|icon-|logo.jpg|warehouse.png).*)",
            "destination": "/index.html"
        }
    ]
}